// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Orphanage {
  id Int @id @default(autoincrement())
  name String
  latitude Float
  longitude Float
  whatsapp String
  about String
  instructions String
  open_on_weekend Boolean
  approved Boolean
}